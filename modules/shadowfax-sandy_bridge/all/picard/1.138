#%Module
proc ModulesHelp { } {
    puts stderr { A set of tools (in Java) for working with next generation sequencing data in the BAM format. - Homepage: http://broadinstitute.github.io/picard/
    }
}

module-whatis {Description: A set of tools (in Java) for working with next generation sequencing data in the BAM format. - Homepage: http://broadinstitute.github.io/picard/}

set root /apps/easybuild/software/shadowfax-sandy_bridge/picard/1.138

conflict picard

if { ![ is-loaded Java/1.7.0_80 ] } {
    module load Java/1.7.0_80
}

prepend-path	CLASSPATH		$root/htsjdk-1.138.jar
prepend-path	CLASSPATH		$root/picard-lib.jar
prepend-path	CLASSPATH		$root/picard.jar
setenv	EBROOTPICARD		"$root"
setenv	EBVERSIONPICARD		"1.138"
setenv	EBDEVELPICARD		"$root/easybuild/picard-1.138-easybuild-devel"

prepend-path	PATH		$root
# Built with EasyBuild version 2.2.0
